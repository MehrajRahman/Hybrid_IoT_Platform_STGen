{
    "name": "Smart Agriculture (Low Power)",
    "description": "Sparse sensors with infrequent updates, low bandwidth",
    "protocol": "YOUR_PROTOCOL",
    "mode": "active",
    "server_ip": "127.0.0.1",
    "server_port": 5000,
    "num_clients": 100,
    "duration": 20,
    "sensors": ["temp", "humidity", "soil_moisture", "light"],
    "traffic_pattern": {
        "temp": {"rate_hz": 0.017, "burst": false},     
        "humidity": {"rate_hz": 0.017, "burst": false},
        "soil_moisture": {"rate_hz": 0.0033, "burst": false},
        "light": {"rate_hz": 0.0083, "burst": false}     
    },
    "network_conditions": {
        "latency_ms": 200,
        "jitter_ms": 50,
        "bandwidth_kbps": 128
    },
    "failure_injection": {
        "packet_loss": 0.05,  
        "client_crashes": []
    }
}