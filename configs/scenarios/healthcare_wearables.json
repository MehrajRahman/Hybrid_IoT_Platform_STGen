{
    "name": "Healthcare Wearable Monitoring",
    "description": "Continuous vital signs monitoring with low latency requirements",
    "protocol": "YOUR_PROTOCOL",
    "mode": "active",
    "server_ip": "127.0.0.1",
    "server_port": 5000,
    "num_clients": 20,
    "duration": 10,
    "sensors": ["heart_rate", "spo2", "temp", "activity"],
    "traffic_pattern": {
        "heart_rate": {"rate_hz": 1.0, "burst": false},  
        "spo2": {"rate_hz": 1.0, "burst": false},
        "temp": {"rate_hz": 0.1, "burst": false},
        "activity": {"rate_hz": 10.0, "burst": true}      
    },
    "qos_requirements": {
        "max_latency_ms": 100,
        "max_loss_percent": 0.5,
        "in_order_delivery": true
    },
    "failure_injection": {
        "packet_loss": 0.0,
        "client_crashes": []
    }
}